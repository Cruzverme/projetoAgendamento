= form_for @agendamento do |f|
  - if @agendamento.errors.any?
    #error_explanation
      %h2= "#{pluralize(@agendamento.errors.count, "error")} prohibited this agendamento from being saved:"
      %ul
        - @agendamento.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :data
    = f.date_select :data
  .field
    = f.label :hora
    = f.time_select :hora
  .field
    = f.label :Estagio
    = f.text_field :Estagio
  .field
    = f.label :Emprego
    = f.text_field :Emprego
  .field
    = f.label :PessoaFisica
    = f.text_field :PessoaFisica
  .actions
    = f.submit 'Save'
